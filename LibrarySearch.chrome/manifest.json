{
  "name": "@EXTENSION_NAME@",
  "version": "@EXTENSION_VERSION@",
  "manifest_version": 2,
  "description": "@EXTENSION_DESCRIPTION@",
  "permissions": [
    "contextMenus",
    "http://*/*", "https://*/*"
  ],
  "background": {
    "scripts": ["content_script.js", "options.js"]
  },
  "icons": {
    "16": "LibrarySearchIcon-16.png",
    "24": "LibrarySearchIcon-24.png",
    "48": "LibrarySearchIcon-48.png"
  },
  "options_page": "options.html"
}

